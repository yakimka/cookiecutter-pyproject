{
  "main_branch": "main",
  "project_name": "my-awesome-project",
  "module_name": "{{ cookiecutter.project_name.lower().replace('-', '_') }}",
  "project_description": "My awesome project",
  "organization": "yakimka",
  "dockerhub_username": "{{ cookiecutter.organization }}",
  "github_username": "{{ cookiecutter.organization }}",
  "minimal_python_version": "3.10",
  "maximum_python_version": "3.14",
  "linter": ["ruff", "flake8"],
  "max_line_length": "88",
  "pydantic_support": false,
  "fastapi_support": false,
  "force_typing": false,
  "dependency_updater": ["dependabot", "renovate", "none"],

  "_copy_without_render": [
    ".venv",
    "docs/_build"
  ]
}
